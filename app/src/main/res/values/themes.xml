<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.InmobiliariaGrupo4" parent="Theme.MaterialComponents.DayNight.DarkActionBar">

        <item name="colorPrimary">@color/aColorPrimary</item>
        <item name="colorPrimaryVariant">@color/aColorPrimaryVariant</item>
        <item name="colorOnPrimary">@color/aColorOnPrimary</item>

        <item name="colorSecondary">@color/aColorSecondary</item>
        <item name="colorSecondaryVariant">@color/aColorSecondaryVariant</item>
        <item name="colorOnSecondary">@color/aColorOnSecondary</item>

        <item name="android:statusBarColor" tools:targetApi="l">@color/aColorPrimaryVariant</item>
        <item name="android:windowBackground">@color/aBackground</item>
    </style>

    <style name="Theme.InmobiliariaGrupo4.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>

    <style name="Theme.InmobiliariaGrupo4.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar" />

    <style name="Theme.InmobiliariaGrupo4.PopupOverlay" parent="ThemeOverlay.AppCompat.Light" />
</resources>